{
  "expo": {
    "name": "Rally",
    "slug": "RallyPreview",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rally-appicon-orange.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "scheme": "rally",
    "splash": {
      "image": "./assets/rally-logo-on-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#131B2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loudlegacy.rally"
    },
    "android": {
      "package": "com.loudlegacy.rally",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/rally-appicon-orange.png",
        "backgroundColor": "#131B2E"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECEIVE_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/rally-icon-white.png",
          "color": "#FF6B35"
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
